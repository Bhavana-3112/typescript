{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "**/10 literal values.ts", // or whichever causes the conflict
    "**/9 union and intersection.ts", // exclude the file causing conflict
    "**/duplicate-definitions-folder/**" // or an entire folder
  ]
}